    "typescript": "~5.3.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~12.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "prettier": "^3.3.3"
